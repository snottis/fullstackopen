{
    "extends": "eslint:recommended",
    "env": {
      "browser": true,
      "jest/globals": true,
      "cypress/globals": true
    },
    "plugins": [
      "react",
      "jest",
      "cypress"
    ],
    "rules": {
      "semi": "error",
      "strict": "error",
      "react/jsx-uses-vars": [2]
    },
    "parserOptions": {
      "ecmaVersion": 12
    },
    "parser": "babel-eslint"
  }